{
  "name": "@safelight/timeline",
  "version": "1.0.0",
  "description": "A Vue component for timelines",
  "author": "Joery MÃ¼nninghoff",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./elements/*": {
      "development": "./src/elements/*.ts",
      "import": "./dist/elements/*.js",
      "types": "./dist/elements/*.d.ts"
    },
    "./elements": {
      "development": "./src/elements/index.ts",
      "import": "./dist/elements/index.js",
      "types": "./dist/elements/index.d.ts"
    },
    "./devtools": {
      "development": "./src/devtools.ts",
      "import": "./dist/devtools.js",
      "types": "./dist/devtools.d.ts"
    }
  },
  "scripts": {
    "dev": "rolldown -w -c",
    "build": "rolldown -c",
    "test": "vitest --browser.headless false",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@vue/devtools-kit": "^7.7.2",
    "@vueuse/core": "^13.0.0",
    "@vueuse/math": "^13.0.0",
    "eventemitter3": "^5.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.14.0",
    "pathe": "^2.0.3",
    "premove": "^4.0.0",
    "rolldown": "1.0.0-beta.9",
    "tinyglobby": "^0.2.12",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "2.2.10"
  }
}
