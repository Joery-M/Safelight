{
  "name": "@safelight/timeline",
  "version": "1.0.0",
  "description": "A Vue component for timelines",
  "author": "Joery MÃ¼nninghoff",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./devtools": {
      "development": "./src/devtools.ts",
      "default": "./dist/devtools.js"
    },
    "./elements": {
      "development": "./src/elements/index.ts",
      "default": "./dist/elements.js"
    },
    "./elements/MoveableTimelineItem": {
      "development": "./src/elements/MoveableTimelineItem.ts",
      "default": "./dist/elements/MoveableTimelineItem.js"
    },
    "./elements/TimelineCursorElement": {
      "development": "./src/elements/TimelineCursorElement.ts",
      "default": "./dist/elements/TimelineCursorElement.js"
    },
    "./elements/TimelineGrid": {
      "development": "./src/elements/TimelineGrid.ts",
      "default": "./dist/elements/TimelineGrid.js"
    },
    "./elements/TimelineLayer": {
      "development": "./src/elements/TimelineLayer.ts",
      "default": "./dist/elements/TimelineLayer.js"
    },
    "./elements/TimelineScrollbar": {
      "development": "./src/elements/TimelineScrollbar.ts",
      "default": "./dist/elements/TimelineScrollbar.js"
    },
    "./elements/VideoTimelineElement": {
      "development": "./src/elements/VideoTimelineElement.ts",
      "default": "./dist/elements/VideoTimelineElement.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./devtools": "./dist/devtools.js",
      "./elements": "./dist/elements.js",
      "./elements/MoveableTimelineItem": "./dist/elements/MoveableTimelineItem.js",
      "./elements/TimelineCursorElement": "./dist/elements/TimelineCursorElement.js",
      "./elements/TimelineGrid": "./dist/elements/TimelineGrid.js",
      "./elements/TimelineLayer": "./dist/elements/TimelineLayer.js",
      "./elements/TimelineScrollbar": "./dist/elements/TimelineScrollbar.js",
      "./elements/VideoTimelineElement": "./dist/elements/VideoTimelineElement.js",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "dev": "tsdown -w",
    "build": "tsdown",
    "test": "vitest --browser.headless false",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@vue/devtools-kit": "^8.0.2",
    "@vueuse/core": "^13.9.0",
    "@vueuse/math": "^13.9.0",
    "eventemitter3": "^5.0.1",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "tsdown": "^0.15.4",
    "vitest": "^3.2.4"
  }
