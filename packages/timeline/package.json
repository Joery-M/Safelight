            ]
        },
        "./*": [
            "./packages/timeline/src/*",
            "./src/*"
        ]
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "dev": "pnpm \"/build:/\" --watch",
        "build": "pnpm \"/build:/\"",
        "build:vite": "vite build --emptyOutDir"
    },
    "dependencies": {
        "@safelight/shared": "workspace:*",
        "@vueuse/core": "^10.9.0",
        "@vueuse/gesture": "^2.0.0",
        "@vueuse/math": "^10.9.0",
        "vue": "3.4.21"
    },
    "devDependencies": {
        "@types/node": "^20.11.30",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "^0.5.1",
        "jsdom": "^24.0.0",
        "typescript": "^5.4.3",
        "vite": "^5.2.4",
        "vite-plugin-dts": "^3.7.3",
        "vitest": "^1.4.0"
