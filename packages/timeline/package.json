{
    "name": "@safelight/timeline",
    "version": "1.0.0",
    "description": "A Vue component for timelines",
    "author": "Joery MÃ¼nninghoff",
    "license": "MIT",
    "type": "module",
    "typings": "dist/src/index.d.ts",
    "main": "src/index.ts",
    "browser": "src/index.ts",
    "module": "src/index.ts",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": [
                "./src/index.ts",
                "./dist/index.mjs"
            ],
            "require": [
                "./src/index.ts",
