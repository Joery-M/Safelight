{
  "name": "@safelight/timeline",
  "version": "1.0.0",
  "description": "A Vue component for timelines",
  "author": "Joery MÃ¼nninghoff",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./elements/*": {
      "development": "./src/elements/*.ts",
      "import": "./dist/elements/*.mjs",
      "types": "./dist/elements/*.d.ts"
    },
    "./elements": {
      "development": "./src/elements/index.ts",
      "import": "./dist/elements/index.mjs",
      "types": "./dist/elements/index.d.ts"
    },
    "./devtools": {
      "development": "./src/devtools.ts",
      "import": "./dist/devtools.mjs",
      "types": "./dist/devtools.d.ts"
    }
  },
  "scripts": {
    "test": "vitest --browser.headless false",
    "test:run": "vitest run",
    "build": "mkdist -d ."
  },
  "dependencies": {
    "@vue/devtools-kit": "^7.7.0",
    "@vueuse/core": "^12.4.0",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/math": "^12.4.0",
    "eventemitter3": "^5.0.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.10.3",
    "@vue/tsconfig": "^0.7.0",
    "mkdist": "^2.2.0",
    "typescript": "^5.7.2",
    "vitest": "^3.0.2",
    "vue-tsc": "^2.2.0"
  }
}
