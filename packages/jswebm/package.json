{
    "name": "@safelight/jswebm",
    "version": "0.0.1",
    "author": "Joery MÃ¼nninghoff",
    "keywords": [
        "webm",
        "matroska"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Joery-M/Safelight"
    },
    "contributors": [],
    "license": "MIT",
    "main": "dist/jswebm.cjs",
    "module": "dist/jswebm.js",
    "types": "dist/JsWebm.d.ts",
    "exports": {
        "./*": {
            "default": "./dist/*.js",
            "require": "./dist/*.cjs",
            "import": "./dist/*.js",
            "types": "./dist/*.d.ts"
        },
        ".": {
            "default": "./dist/jswebm.js",
            "require": "./dist/jswebm.cjs",
            "import": "./dist/jswebm.js",
            "types": "./dist/JsWebm.d.ts"
        }
    },
    "type": "module",
    "scripts": {
        "dev": "vite build --mode=development --watch",
        "build": "rimraf ./dist/ && pnpm build:vite && pnpm build:tsc",
        "build:vite": "vite build",
        "build:tsc": "tsc --emitDeclarationOnly --project tsconfig.lib.json",
        "generate:elements": "cd ./tools/ && tsx ./SchemaParser.ts https://raw.githubusercontent.com/ietf-wg-cellar/matroska-specification/master/ebml_matroska.xml && prettier ../src/elements.ts --write"
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "fast-xml-parser": "^4.4.0",
        "rimraf": "^5.0.5",
        "terser": "^5.31.3",
        "tsx": "^4.16.0",
        "typescript": "^5.5.3",
        "vite": "^5.3.2",
        "vite-plugin-dts": "^3.9.1",
        "vite-plugin-no-bundle": "^4.0.0"
    },
    "dependencies": {
        "tseep": "^1.2.2"
    }
}
