{
    "name": "@safelight/tswebm",
    "version": "1.0.0",
    "author": "Joery MÃ¼nninghoff",
    "keywords": [
        "webm",
        "matroska"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Joery-M/Safelight"
    },
    "contributors": [],
    "license": "MIT",
    "main": "dist/tswebm.cjs",
    "module": "dist/tswebm.js",
    "types": "dist/tswebm.d.ts",
    "exports": {
        "./*": {
            "default": "./dist/*.js",
            "require": "./dist/*.cjs",
            "import": "./dist/*.js",
            "types": "./dist/*.d.ts"
        },
        ".": {
            "default": "./dist/tswebm.js",
            "require": "./dist/tswebm.cjs",
            "import": "./dist/tswebm.js",
            "types": "./dist/tswebm.d.ts"
        }
    },
    "type": "module",
    "scripts": {
        "dev": "vite build --mode=development --watch",
        "clean": "rimraf ./dist/ ./tsconfig.lib.tsbuildinfo",
        "build": "pnpm run \"/(clean|generate:elements)/\" && pnpm build:vite && pnpm build:tsc",
        "build:vite": "vite build",
        "build:tsc": "tsc --emitDeclarationOnly --project tsconfig.lib.json",
        "generate:elements": "tsx ./tools/SchemaParser.ts https://raw.githubusercontent.com/ietf-wg-cellar/matroska-specification/master/ebml_matroska.xml && prettier ./src/elements.ts --write",
        "test": "vitest",
        "test:run": "vitest run --ui false"
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "change-case": "^5.4.4",
        "fast-xml-parser": "^4.4.0",
        "happy-dom": "^14.12.3",
        "rimraf": "^5.0.5",
        "terser": "^5.31.3",
        "tseep": "^1.2.2",
        "tsx": "^4.16.0",
        "typescript": "^5.5.3",
        "vite": "^5.3.2",
        "vite-plugin-dts": "^3.9.1",
        "vitest": "^2.0.4"
    }
}
