{
  "name": "@safelight/tswebm",
  "version": "1.0.0",
  "author": "Joery MÃ¼nninghoff",
  "keywords": [
    "webm",
    "matroska"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Joery-M/Safelight"
  },
  "contributors": [],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./block": {
      "development": "./src/block.ts",
      "default": "./dist/block.js"
    },
    "./dataReader": {
      "development": "./src/dataReader.ts",
      "default": "./dist/dataReader.js"
    },
    "./elements": {
      "development": "./src/elements.ts",
      "default": "./dist/elements.js"
    },
    "./WebmReader": {
      "development": "./src/WebmReader.ts",
      "default": "./dist/WebmReader.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./block": "./dist/block.js",
      "./dataReader": "./dist/dataReader.js",
      "./elements": "./dist/elements.js",
      "./WebmReader": "./dist/WebmReader.js",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "dev": "tsdown -w",
    "build": "pnpm generate:elements && pnpm build:no-gen",
    "build:no-gen": "tsdown",
    "generate:elements": "tsx ./tools/SchemaParser.ts https://raw.githubusercontent.com/ietf-wg-cellar/matroska-specification/master/ebml_matroska.xml && prettier ./src/elements.ts --write --print-width 110",
    "test": "vitest --ui",
    "test:run": "vitest run --ui false"
  },
  "devDependencies": {
    "@types/node": "^22.18.6",
    "@vitest/ui": "^3.2.4",
    "change-case": "^5.4.4",
    "eventemitter3": "^5.0.1",
    "fast-xml-parser": "^5.2.5",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
